local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "chatbypasser v1", HidePremium = false, IntroText = "chatbypasser v1 loading", SaveConfig = true, ConfigFolder = "chatbypasser"})

local Tab = Window:MakeTab({
	Name = "fun",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "I LOVE BUTT NAKED MINORS",
	Callback = function()
        local TextChatService = game:GetService("TextChatService")
local Players = game:GetService("Players")
local generalChannel : TextChannel = TextChatService:WaitForChild("TextChannels"):WaitForChild("RBXGeneral")

local blockLetters = {
    A = "ğŸ…", B = "ğŸ…‘", C = "ğŸ…’", D = "ğŸ…“", E = "ğŸ…”",
    F = "ğŸ…•", G = "ğŸ…–", H = "ğŸ…—", I = "ğŸ…˜", J = "ğŸ…™",
    K = "ğŸ…š", L = "ğŸ…›", M = "ğŸ…œ", N = "ğŸ…", O = "ğŸ…",
    P = "ğŸ…Ÿ", Q = "ğŸ… ", R = "ğŸ…¡", S = "ğŸ…¢", T = "ğŸ…£",
    U = "ğŸ…¤", V = "ğŸ…¥", W = "ğŸ…¦", X = "ğŸ…§", Y = "ğŸ…¨",
    Z = "ğŸ…©",
    ["0"] = "â“ª", ["1"] = "â‘ ", ["2"] = "â‘¡", ["3"] = "â‘¢", ["4"] = "â‘£",
    ["5"] = "â‘¤", ["6"] = "â‘¥", ["7"] = "â‘¦", ["8"] = "â‘§", ["9"] = "â‘¨",
    ["/"] = "ï¼",
    ["."] = " â€¢ ",
    [":"] = "ï¸°"
}

local function toBlockCode(input)
    local output = ""
    for i = 1, #input do
        local char = input:sub(i, i):upper()
        output = output .. (blockLetters[char] or char)
    end
    return output
end

generalChannel:SendAsync(toBlockCode("I LOVE BUTT NAKED MINORS"))
  	end    
})

